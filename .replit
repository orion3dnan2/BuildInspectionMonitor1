modules = ["php-8.2"]
[agent]
expertMode = true

[userenv]

[userenv.shared]
APP_NAME = "نظام التفتيش والمراقبة"
APP_ENV = "local"
APP_DEBUG = "true"
APP_URL = "http://0.0.0.0:5000"
APP_LOCALE = "ar"
APP_FALLBACK_LOCALE = "en"
APP_FAKER_LOCALE = "ar_SA"
DB_CONNECTION = "pgsql"
SESSION_DRIVER = "database"
SESSION_LIFETIME = "120"
CACHE_STORE = "database"
QUEUE_CONNECTION = "database"

[workflows]
runButton = "Project"

[[workflows.workflow]]
name = "Project"
mode = "parallel"
author = "agent"

[[workflows.workflow.tasks]]
task = "workflow.run"
args = "Laravel Server"

[[workflows.workflow]]
name = "Laravel Server"
author = "agent"

[[workflows.workflow.tasks]]
task = "shell.exec"
args = "php artisan serve --host=0.0.0.0 --port=5000"
waitForPort = 5000

[workflows.workflow.metadata]
outputType = "webview"

[[ports]]
localPort = 5000
externalPort = 80

[[ports]]
localPort = 33489
externalPort = 3000
